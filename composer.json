{
    "name": "dulithaks/whatsapp",
    "description": "WhatsApp Cloud API integration for Laravel - Send and receive messages, media, and handle webhooks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Duli Thakshila",
            "email": "dulithaks@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Duli\\WhatsApp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Duli\\WhatsApp\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Duli\\WhatsApp\\WhatsAppServiceProvider"
            ],
            "aliases": {
                "WhatsApp": "Duli\\WhatsApp\\Facades\\WhatsApp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
